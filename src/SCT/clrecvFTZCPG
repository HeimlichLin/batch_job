RunTx()
{
#. /BATCH_JOB/def/SET_ENV.deff
$JAVA_HOME7/bin/java com.batchJob.common.dequeue.cpg.CPGDequeueFTZL
}

#--------------------------------------------------------------------#
#                            MAIN ROUTINE                            #
#--------------------------------------------------------------------#
while true
do
	echo 'CPGDequeueFTZL run ......'
  #解析檔案
  RunTx
  
  echo 'CPGDequeueFTZL sleeping ......'
  sleep 180
done